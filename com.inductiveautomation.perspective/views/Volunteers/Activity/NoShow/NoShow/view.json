{
  "custom": {
    "Data": [
      {
        "hours": "",
        "title": "",
        "venue": ""
      }
    ]
  },
  "params": {
    "Date": "",
    "ID": 0,
    "Size": 110
  },
  "propConfig": {
    "custom.Data": {
      "binding": {
        "config": {
          "parameters": {
            "instanceId": "{view.params.ID}"
          },
          "queryPath": "Calendar/GetEventByInstanceID",
          "returnFormat": "json"
        },
        "transforms": [
          {
            "code": "\tif len(value):\n\t\treturn value\n\telse:\n\t\treturn [{\n\t  \"title\": \"\",\n\t  \"venue\": \"\",\n\t  \"hours\": \"\"\n\t}]",
            "type": "script"
          }
        ],
        "type": "query"
      },
      "persistent": true
    },
    "params.Date": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.ID": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.Size": {
      "binding": {
        "config": {
          "path": "view.props.defaultSize.height"
        },
        "type": "property"
      },
      "paramDirection": "output",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 110
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "EmbeddedView"
        },
        "position": {
          "basis": "40px",
          "shrink": 0
        },
        "propConfig": {
          "props.params.Date": {
            "binding": {
              "config": {
                "expression": "{view.params.Date}"
              },
              "transforms": [
                {
                  "expression": "dateFormat({value},\"MMM. d, YYYY\") +\" at \"+ dateFormat({value},\"hh:mm a\")",
                  "type": "expression"
                }
              ],
              "type": "expr"
            }
          }
        },
        "props": {
          "params": {
            "Icon": "material/highlight_off",
            "Title": "No Show"
          },
          "path": "Volunteers/Activity/ActivityHeader"
        },
        "type": "ia.display.view"
      },
      {
        "meta": {
          "name": "EmbeddedView_1"
        },
        "position": {
          "basis": "64px",
          "shrink": 0
        },
        "propConfig": {
          "props.params.time": {
            "binding": {
              "config": {
                "path": "view.custom.Data[0].minutes"
              },
              "transforms": [
                {
                  "code": "\tif not value:\n\t\tvalue \u003d 0\n\telse:\n\t\tvalue \u003d float(value)/60\n\treturn value",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          },
          "props.params.title": {
            "binding": {
              "config": {
                "path": "view.custom.Data[0].title"
              },
              "type": "property"
            }
          },
          "props.params.venue": {
            "binding": {
              "config": {
                "path": "view.custom.Data[0].venue"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "path": "Volunteers/Activity/NoShow/InformationTop",
          "style": {
            "borderBottomColor": "#e6e6e6",
            "borderBottomStyle": "none",
            "borderBottomWidth": 1,
            "borderTopColor": "#e6e6e6",
            "borderTopStyle": "solid",
            "borderTopWidth": 1,
            "marginLeft": "50px",
            "marginRight": "50px",
            "paddingTop": "5px"
          }
        },
        "type": "ia.display.view"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "classes": "Background/Default",
        "overflow": "hidden"
      }
    },
    "type": "ia.container.flex"
  }
}