{
  "custom": {},
  "params": {
    "Date": 0,
    "ID": 0,
    "Size": 50
  },
  "propConfig": {
    "params.Date": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.ID": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.Size": {
      "binding": {
        "config": {
          "path": "view.props.defaultSize.height"
        },
        "type": "property"
      },
      "paramDirection": "output",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 50
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "EmbeddedView"
        },
        "position": {
          "basis": "40px",
          "shrink": 0
        },
        "propConfig": {
          "props.params.Date": {
            "binding": {
              "config": {
                "expression": "{view.params.Date}"
              },
              "transforms": [
                {
                  "expression": "dateFormat({value},\"MMM. d, YYYY\") +\" at \"+ dateFormat({value},\"hh:mm a\")",
                  "type": "expression"
                }
              ],
              "type": "expr"
            }
          }
        },
        "props": {
          "params": {
            "Icon": "material/add_task",
            "Title": "Added to Volunteers"
          },
          "path": "Volunteers/Activity/ActivityHeader"
        },
        "type": "ia.display.view"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "justify": "center",
      "style": {
        "classes": "Background/Default",
        "overflow": "hidden"
      }
    },
    "type": "ia.container.flex"
  }
}