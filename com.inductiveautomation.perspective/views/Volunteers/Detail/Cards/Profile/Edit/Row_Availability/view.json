              },
              "props.params.selection": {
                "binding": {
                  "config": {
                    "bidirectional": true,
                    "path": "view.params.availability_seasonal_availability_type"
                  },
                  "type": "property"
                },
                "onChange": {
                  "enabled": null,
                  "script": "\tif self.props.params.selection \u003d\u003d 1:\n\t\tself.view.params.availability_seasonal_start_date \u003d \u0027\u0027\n\t\tself.view.params.availability_seasonal_end_date \u003d \u0027\u0027\n\t\tself.view.params.availability_weekly_selection \u003d \u0027\u0027 "
                }
              }
            },
            "props": {
              "params": {
                "text": "My Availablity Is..."
              },
              "path": "Volunteers/Templates/60px/top/Input_DropDownSingle",
