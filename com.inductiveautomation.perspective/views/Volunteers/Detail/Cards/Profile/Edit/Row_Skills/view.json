{
  "custom": {
    "skills": [
      {
        "label": "Addiction Support",
        "value": 0
      },
      {
        "label": "Food Distribution",
        "value": 1
      },
      {
        "label": "Shelter Assistance",
        "value": 2
      },
      {
        "label": "Counseling Support",
        "value": 4
      },
      {
        "label": "Clothing Management",
        "value": 5
      },
      {
        "label": "Job Readiness Training",
        "value": 6
      }
    ]
  },
  "params": {
    "skills_selection": null
  },
  "propConfig": {
    "custom.skills": {
      "binding": {
        "config": {
          "queryPath": "Volunteers/Skills/SkillsNamesIds",
          "returnFormat": "json"
        },
        "transforms": [
          {
            "code": "\tselections \u003d []\n\tfor row in value:\n\t\tselections.append({\"value\": row[\"id\"], \"label\": row[\"skillName\"]})\n\treturn selections",
            "type": "script"
          }
        ],
        "type": "query"
      },
      "persistent": true
    },
    "params.skills_selection": {
      "paramDirection": "inout",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 60,
      "width": 1416
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Input_DropDownMulti"
        },
        "position": {
          "basis": "100%"
        },
        "propConfig": {
          "props.params.options": {
            "binding": {
              "config": {
                "path": "view.custom.skills"
              },
              "type": "property"
            }
          },
          "props.params.selection": {
            "binding": {
              "config": {
                "bidirectional": true,
                "path": "view.params.skills_selection"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "params": {
            "text": "Skills*"
          },
          "path": "Volunteers/Templates/60px/top/Input_DropDownMulti",
          "style": {
            "classes": "InputCards/InputCard"
          }
        },
        "type": "ia.display.view"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "style": {
        "classes": "Background/Default"
      }
    },
    "type": "ia.container.flex"
  }
}