{
  "custom": {
    "status": [
      {
        "label": "Community Cleanup",
        "value": 2
      },
      {
        "label": "Benefit concert",
        "value": 4
      }
    ]
  },
  "params": {
    "events_selections": null
  },
  "propConfig": {
    "custom.status": {
      "binding": {
        "config": {
          "queryPath": "Calendar/GetEventsNew",
          "returnFormat": "json"
        },
        "transforms": [
          {
            "code": "\tselections \u003d []\n\tfor row in value:\n\t\tselections.append({\"value\": row[\"eventID\"], \"label\": row[\"title\"]})\n\treturn selections",
            "type": "script"
          }
        ],
        "type": "query"
      },
      "persistent": true
    },
    "params.events_selections": {
      "paramDirection": "inout",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 60,
      "width": 1043
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Embed_Status"
        },
        "position": {
          "basis": "100%"
        },
        "propConfig": {
          "props.params.options": {
            "binding": {
              "config": {
                "path": "view.custom.status"
              },
              "type": "property"
            }
          },
          "props.params.selection": {
            "binding": {
              "config": {
                "bidirectional": true,
                "path": "view.params.events_selections"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "params": {
            "enabled": false,
            "text": "Events"
          },
          "path": "Volunteers/Templates/Application/Input_DropDownMulti",
          "style": {
            "classes": "InputCards/InputCard"
          }
        },
        "type": "ia.display.view"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "style": {
        "classes": "Background/Default"
      }
    },
    "type": "ia.container.flex"
  }
}