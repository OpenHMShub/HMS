  "propConfig": {
    "custom.notes_data": {
      "binding": {
        "config": {
          "parameters": {
            "volunteerId": "{view.params.trackID}"
          },
          "queryPath": "Volunteers/Notes/NotesVolunteerSelect",
          "returnFormat": "dataset"
        },
        "transforms": [
          {
            "code": "\t\"\"\"\n\t\t\t####THIS SECTION FOR INFORMATION ONLY####\n\t\t\t#Example Drug Screen Detail data object should look like this:\n\t\t\t{\n\t\t\t\t\"value\": {\n\t\t\t\t  \"id\": 0\n\t\t\t\t  \"employee_id\":0\n\t\t\t\t  \"employee_name\": \"Susan Jones\"\n\t\t\t\t  \"hmis\":False\n\t\t\t\t  \"note_trunc\":\"I really like th...\"\n\t\t\t\t  \"note\":\"I really like this participant\"\t\t\t  \n\t\t\t\t  \"time_created\": \"07/30/2020\",\n\t\t\t\t \t }\n\t\t\t\t\"style\": {\n\t\t\t\t      \"classes\" \u003d \"Table/Cell_Detail\"\n\t\t\t\t\t  }\n\t\t\t\t}\n\t\t####END INFORMATION####\n\t\t\t\n\t\t####BEGIN ACTUAL SCRIPT####\n\t\"\"\"\n\t#table_data is the data object to return\n\ttable_data \u003d []\n\t\t\n\t#Iterate over the results of the query\n\tfor row in range(value.getRowCount()):\n\t\trow_struct \u003d {}\n\t\trow_data \u003d {}\n\t\tstyle \u003d {}\t\n\t\t#Map query results into correct attributes\n\t\trow_data[\u0027note_id\u0027] \u003d value.getValueAt(row, \"id\")\n\t\trow_data[\u0027time_created\u0027] \u003d value.getValueAt(row, \"timeCreated\")\t\n\t\trow_data[\u0027volunteerId\u0027] \u003d value.getValueAt(row, \"volunteerId\")\n\t\trow_data[\u0027note_trunc\u0027] \u003d value.getValueAt(row, \"note\")[0:80]\n\t\trow_data[\u0027note\u0027] \u003d value.getValueAt(row, \"note\")\n\t\t#Set the default style for the row\n\t\tstyle[\u0027classes\u0027] \u003d \"Table/Cell_Detail\"\n\t\trow_struct[\u0027value\u0027] \u003d row_data\n\t\trow_struct[\u0027style\u0027] \u003d style\n\t\t#Append the individual row date to the table data object\n\t\ttable_data.append(row_struct)\n\t\t\t\t\n\treturn table_data",
            "type": "script"
          }
        ],
        "type": "query"
      },
      "persistent": true
    },
    "custom.table_data": {
      "binding": {
        "config": {
          "path": "view.custom.notes_data"
        },
        "transforms": [
          {
            "code": "\tnotes_data \u003d value\n\ttable_data \u003d []\n\t#Move the date from the case notes directly to the table\n\tif len(notes_data):\n\t\tfor row in range(len(notes_data)):\n\t\t\trow_struct \u003d {}\n\t\t\tnote \u003d {}\n\t\t\tstyle \u003d {}\t\n\t\t\t#Map query results into correct attributes\n\t\t\t#system.perspective.print(\u0027Table Data Processing Note\u0027 + str(notes_data[row].value.note_id))\n\t\t\tnote[\u0027note_id\u0027] \u003d notes_data[row].value.note_id\n\t\t\tnote[\u0027time_created\u0027] \u003d notes_data[row].value.time_created\n\t\t\tnote[\u0027volunteerId\u0027] \u003d notes_data[row].value.volunteerId\n\t\t\tnote[\u0027note\u0027] \u003d notes_data[row].value.note\n\t\t\tnote[\u0027note_trunc\u0027] \u003d notes_data[row].value.note_trunc\n\n\t\t\t#Set the default style for the row\n\t\t\tstyle[\u0027classes\u0027] \u003d \"Table/Cell_Detail\"\n\t\t\trow_struct[\u0027value\u0027] \u003d note\n\t\t\trow_struct[\u0027style\u0027] \u003d style\n\t\t\t#Append the individual row date to the table data object\n\t\t\ttable_data.append(row_struct)\n\t\t\n\treturn table_data",
            "type": "script"
          }
        ],
        "type": "property"
      },
      "onChange": {
        "enabled": null,
        "script": "\t#system.perspective.print(\"Table Data Changed\")\n\tsystem.perspective.sendMessage(messageType\u003d\"refreshNotesDetail\", payload\u003d{}, scope\u003d\"session\")"
      },
      "persistent": true
    },
