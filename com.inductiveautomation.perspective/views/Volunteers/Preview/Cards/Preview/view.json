    "propConfig": {
      "custom.lastAssignment": {
        "binding": {
          "config": {
            "parameters": {
              "volunteerId": "{view.params.trackID}"
            },
            "queryPath": "Volunteers/Details/VolunteerLastAssignment",
            "returnFormat": "json"
          },
          "transforms": [
            {
              "code": "\tif len(value) \u003c 1:\n\t\treturn {}\n\telse:\n\t\tvalue \u003d value[0]\n\treturn value",
              "type": "script"
            }
          ],
          "type": "query"
        }
      },
      "custom.volunteer": {
        "binding": {
          "config": {
            "parameters": {
              "volunteerId": "{view.params.trackID}"
            },
            "queryPath": "Volunteers/Details/PersonalInfo",
            "returnFormat": "json"
          },
          "transforms": [
            {
              "code": "\tdef safe_get(obj, attr, default\u003d\"Not collected\"):\n\t    value \u003d getattr(obj, attr, None)\n\t    if value is None or value \u003d\u003d \"\" or value \u003d\u003d \" \":\n\t        return default\n\t    return value\n\t\n\tif len(value) \u003c 1:\n\t\treturn {}\n\telse:\n\t\tdata \u003d value[0]\n\t\tpersonalInformation \u003d {\n\t\t    \"id\": safe_get(data, \"id\"),  \n\t\t    \"firstName\": safe_get(data, \"firstName\"),\n\t\t    \"middleName\": safe_get(data, \"middleName\"),\n\t\t    \"lastName\": safe_get(data, \"lastName\"),\n\t\t    \"genderName\": safe_get(data, \"genderName\"),\n\t\t    \"dob\": safe_get(data, \"dob\"),\n\t\t    \"age\": safe_get(data, \"age\"),\n\t\t    \"phone\": safe_get(data, \"phone\"),\n\t\t    \"email\": safe_get(data, \"email\"),\n\t\t    \"employer\": safe_get(data, \"employer\"),\n\t\t    \"school\": safe_get(data, \"school\"),\n\t\t    \"street\": safe_get(data, \"street\"),\n\t\t    \"city\": safe_get(data, \"city\"),\n\t\t    \"state\": safe_get(data, \"state\"),\n\t\t    \"zipCode\": safe_get(data, \"zipCode\"),  \n\t\t    \"congregation\": safe_get(data, \"congregation\"),\n\t\t    \"hours\": safe_get(data, \"hours\"),\n\t\t    \"volunteerGroupName\": safe_get(data, \"volunteerGroupName\")\n\t\t}\n\n\treturn personalInformation",
              "type": "script"
            }
          ],
          "type": "query"
        }
