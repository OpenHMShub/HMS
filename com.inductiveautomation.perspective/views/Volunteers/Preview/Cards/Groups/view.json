          "grow": 1
        },
        "type": "ia.display.label"
      },
      {
        "events": {
          "dom": {
            "onClick": {
              "config": {
                "script": "\tself.view.params.More \u003d not self.view.params.More "
              },
              "scope": "G",
              "type": "script"
            }
          }
        },
        "meta": {
          "name": "Label"
        },
        "position": {
          "basis": "32px",
          "shrink": 0
        },
        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "expression": "if(isNull({view.custom.group}),false,true)"
              },
              "type": "expr"
            }
          },
          "props.text": {
            "binding": {
              "config": {
                "expression": "if({view.params.More}, \u0027LESS\u0027, \u0027MORE\u0027)"
              },
              "type": "expr"
            }
          }
        },
        "props": {
          "style": {
            "classes": "Text/About_info",
            "paddingLeft": "20px",
            "textDecoration": "underline"
          }
        },
        "type": "ia.display.label"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "classes": "Background/Default"
      }
    },
    "type": "ia.container.flex"
  }
