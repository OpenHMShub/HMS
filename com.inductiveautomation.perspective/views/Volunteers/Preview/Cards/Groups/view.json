        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "expression": "if({view.params.groupNull},False,if({view.params.More},True, False)) "
              },
              "type": "expr"
            }
          },
          "props.params.Info": {
            "binding": {
              "config": {
