{
  "custom": {
    "group": {
      "MTDHours": 6,
      "MemberCount": 3,
      "Name": "Event Support",
      "TotalHours": 6,
      "YTDHours": 6,
      "description": "Volunteers helping organize and run events"
    }
  },
  "params": {
    "More": false,
    "groupNull": false,
    "trackID": 50
  },
  "propConfig": {
    "custom.group": {
      "binding": {
        "config": {
          "parameters": {
            "volunteerID": "{view.params.trackID}"
          },
          "queryPath": "Volunteers/Group/groupDetail",
          "returnFormat": "json"
        },
        "transforms": [
          {
            "code": "\tif value:\n\t\tdata \u003d value[0]\n\t\tgroup \u003d {\n\t\t    \"MTDHours\": data.MTDHours,\n\t\t    \"MemberCount\": data.MemberCount,\n\t\t    \"Name\": data.Name,\n\t\t    \"TotalHours\": data.TotalHours,\n\t\t    \"YTDHours\": data.YTDHours,\n\t\t    \"description\": data.description\n\t\t}\n\t\treturn group\n\telse:\n\t\treturn None",
            "type": "script"
          }
        ],
        "type": "query"
      },
      "persistent": true
    },
    "params.More": {
      "paramDirection": "output",
      "persistent": true
    },
    "params.groupNull": {
      "binding": {
        "config": {
          "expression": "isNull({view.custom.group})"
        },
        "type": "expr"
      },
      "paramDirection": "output",
      "persistent": true
    },
    "params.trackID": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 128,
      "width": 600
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "NoGroupMessage"
        },
        "position": {
          "basis": "32px",
          "grow": 1
        },
        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "expression": "if(isNull({view.custom.group}),true,false)"
              },
              "type": "expr"
            }
          }
        },
        "props": {
          "alignVertical": "top",
          "style": {
            "classes": "Text/About_info"
          },
          "text": "The volunteer is not part of any group"
        },
        "type": "ia.display.label"
      },
      {
        "meta": {
          "name": "EmbeddedView"
        },
        "position": {
          "basis": "45px",
          "shrink": 0
        },
        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "expression": "if(isNull({view.custom.group}),false,true)"
              },
              "type": "expr"
            }
          },
          "props.params.Info": {
            "binding": {
              "config": {
                "expression": "if(isnull({view.custom.group.Name}),\"NONE\", {view.custom.group.Name})"
              },
              "type": "expr"
            }
          }
        },
        "props": {
          "params": {
            "Description": "Group name\n"
          },
          "path": "Volunteers/Detail/Cards/Personal/AboutLabel",
          "useDefaultViewHeight": true
        },
        "type": "ia.display.view"
      },
      {
        "meta": {
          "name": "EmbeddedView_0"
        },
        "position": {
          "basis": "45px",
          "shrink": 0
        },
        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "expression": "if(isNull({view.custom.group}),false,true)"
              },
              "type": "expr"
            }
          },
          "props.params.Info": {
            "binding": {
              "config": {
                "expression": "if(isnull({view.custom.group.description}),\"NONE\", {view.custom.group.description})"
              },
              "type": "expr"
            }
          }
        },
        "props": {
          "params": {
            "Description": "Group description"
          },
          "path": "Volunteers/Detail/Cards/Personal/AboutLabel",
          "useDefaultViewHeight": true
        },
        "type": "ia.display.view"
