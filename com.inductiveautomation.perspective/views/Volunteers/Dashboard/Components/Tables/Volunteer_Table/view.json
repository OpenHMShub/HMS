            },
            {
              "messageType": "changeCheckout",
              "pageScope": true,
              "script": "\t# implement your handler here\n\tnewCheckout \u003d payload.get(\"checkout\")\n\tnewCheckin \u003d payload.get(\"checkin\")\n\trow \u003d payload[\"row\"]\n\t\n\tevtAttId \u003d self.props.data[row][\"EventAttendeeID\"] \n\tsystem.perspective.print(evtAttId)\n\tsystem.perspective.print(newCheckout)\n\tif newCheckout is not None:\n\t\tsystem.db.runPrepUpdate(\"Update calendar.EventAttendance set checkout \u003d ? where id \u003d?\", [newCheckout, evtAttId])\n\telif newCheckin is not None:\n\t\tsystem.db.runPrepUpdate(\"Update calendar.EventAttendance set checkin \u003d ? where id \u003d?\", [newCheckin, evtAttId])\n\t\n\tself.refreshBinding(\"custom.sqlData\")",
              "sessionScope": false,
