    "scripts": {
      "customMethods": [
        {
          "name": "sendMsgCondition",
          "params": [
            "payload\u003dNone"
          ],
          "script": "\t\n\temvRoom \u003d self.getChild(\"emvRoom\")\n\temvVersion\u003d self.getChild(\"emvVersion\")\n\tif payload is None:\n\t\tpayload \u003d {}\n\t\tpayload[\u0027oneDay\u0027] \u003d system.date.now()\n\t\t#payload[\u0027facilityId\u0027] \u003d self.view.params.facilityId\n\t\tpayload[\u0027roomId\u0027] \u003d emvRoom.props.params.selection\n\t\tpayload[\u0027ver\u0027] \u003d emvVersion.props.params.version\n\n\tsystem.perspective.print((\u0027mhByCondition\u0027,payload)) \n\t\n\tsystem.perspective.sendMessage(\u0027mhByCondition\u0027, payload \u003d payload)\n\t\n"
        },
        {
          "name": "setPropsDropdowns",
          "params": [],
          "script": "\temvRoom \u003d self.getChild(\"emvRoom\")\n\tquery \u003d \"\"\"\nSELECT id\n\t,roomName\nFROM HMSOps.lodging.room\nWHERE timeRetired IS NULL\nORDER BY facilityId, roomName\"\"\"\t\t\t\n\t\n\tresult \u003d system.db.runPrepQuery(query,[],\u0027HMSOps\u0027)\n\temvRoom.props.params.options \u003d [{\u0027value\u0027:row[\u0027id\u0027],\u0027label\u0027:row[\u0027roomName\u0027]} for row in result]\n\t"
        }
      ],
      "extensionFunctions": null,
      "messageHandlers": [
        {
          "messageType": "refreshProviderTableFromFilter",
          "pageScope": true,
          "script": "\tself.sendMsgCondition()",
          "sessionScope": false,
          "viewScope": false
        }
      ]
