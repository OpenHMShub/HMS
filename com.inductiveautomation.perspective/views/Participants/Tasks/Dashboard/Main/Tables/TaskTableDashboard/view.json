        "events": {
          "component": {
            "onRowClick": {
              "config": {
                "script": "\t\n\tselectedColumn \u003d self.props.selection.selectedColumn\n\tif selectedColumn \u003d\u003d \"participantId\": \n\t\tid \u003d \"PreviewParticipant\" \n\t\tparticipantId \u003d self.props.selection.data[0].participantId\n\t\tparams \u003d {\"participantID\":participantId}\n\t\tif self.custom.DockID !\u003d participantId:\n\t\t\tsystem.perspective.openDock(id\u003did,params\u003dparams)\n\t\t\tpayload \u003d {\"participantID\":participantId}\n\t\t\tsystem.perspective.sendMessage(\"DockUpdate\", payload\u003dpayload, scope \u003d \u0027page\u0027 )\n\t\telse:\n\t\t\tsystem.perspective.toggleDock(id\u003did,params\u003dparams)\n\t\tself.custom.DockID \u003d participantId\n\telse:\n\t\trow \u003d self.props.data[event.row]\n\t\tselectedRow \u003d {\n\t\t  \"Task Type\": row[\"Task Type\"],\n\t\t  \"assignerName\": row[\"assignerName\"],\n\t\t  \"Task\": row[\"Task\"],\n\t\t  \"notes\": row[\"notes\"],\n\t\t  \"Participant Name\": row[\"Participant Name\"],\n\t\t  \"completed\":row[\"completed\"],\n\t\t  \"priority\": row[\"priority\"],\n\t\t  \"Created\": row[\"Created\"],\n\t\t  \"priorityId\": row[\"priorityId\"],\n\t\t  \"participantId\": row[\"participantId\"],\n\t\t  \"taskTypeId\": row[\"taskTypeId\"],\n\t\t  \"statusId\": row[\"statusId\"],\n\t\t  \"staffName\": row[\"staffName\"],\n\t\t  \"id\": row[\"id\"],\n\t\t  \"staffId\": row[\"staffId\"],\n\t\t  \"Due Date\": row[\"Due Date\"][\u0027value\u0027],\n\t\t  \"assignerId\": row[\"assignerId\"],\n\t\t  \"status\": row[\"status\"]\n\t\t}\n#\t\trow[\u0027Due Date\u0027]\u003drow[\u0027Due Date\u0027][\u0027value\u0027]\n#\t\tsystem.perspective.print(selectedRow)\n\t\tself.view.params.selectedRowData  \u003d selectedRow\n\t"
              },
              "scope": "G",
              "type": "script"
            }
          }
        },
        "meta": {
          "name": "Table"
        },
        "position": {
          "basis": "100%",
          "grow": 1
        },
        "propConfig": {
          "props.data": {
            "binding": {
              "config": {
                "struct": {
                  "data": "{view.custom.sql_data}",
                  "showAllTasks": "{view.params.showAllTasks}",
                  "staffOnly": "{view.params.staffOnly}"
                },
                "waitOnAll": true
              },
              "transforms": [
                {
                  "code": "\ttable_data \u003d []\n\tloggedInUser \u003d self.session.custom.loggedInStaffEmployeeID\n\tshowAllTasks \u003d value[\u0027showAllTasks\u0027]\n\tstaffOnly \u003d value[\u0027staffOnly\u0027]\n\tvalue \u003d value[\u0027data\u0027]\n\t#Iterate over the results of the query\n\tfor row in range(value.getRowCount()):\n\t\trow_data \u003d {}\n\t\tstyle \u003d {}\n\t\tstyle[\u0027classes\u0027] \u003d \"Table/Cell_Dashboard\"\n\t\t#Map query results into correct attributes\n\t\trow_data[\u0027id\u0027] \u003d value.getValueAt(row, \"id\")\n\t\t#row_data[\u0027options\u0027] \u003d \u0027\u0027\n\t\trow_data[\u0027Task Type\u0027] \u003d value.getValueAt(row, \"type\")\n\t\trow_data[\u0027Task\u0027] \u003d value.getValueAt(row, \"title\")\n\t\trow_data[\u0027Participant Name\u0027] \u003d value.getValueAt(row, \"participantName\")\n\t\tif value.getValueAt(row, \"dueDate\")\u003esystem.date.now():\n\t\t\tDateColor \u003d \u0027#0C4E8B\u0027\n\t\telse:\n\t\t\tDateColor \u003d \u0027#FF7A59\u0027\n\t\trow_data[\u0027Due Date\u0027] \u003d {\u0027value\u0027:value.getValueAt(row, \"dueDate\"),\u0027style\u0027:{\u0027color\u0027:DateColor}}\n\t\trow_data[\u0027Created\u0027] \u003d system.date.format(value.getValueAt(row, \"timeCreated\"), \"MM/dd/YYYY hh:mm a\")\n\t\trow_data[\u0027notes\u0027] \u003d value.getValueAt(row, \"notes\")\n\t\trow_data[\u0027participantId\u0027] \u003d value.getValueAt(row, \"participantId\")\n\t\trow_data[\u0027taskTypeId\u0027] \u003d value.getValueAt(row, \"taskTypeId\")\n\t\trow_data[\u0027staffId\u0027] \u003d value.getValueAt(row, \"staffId\")\n\t\trow_data[\u0027statusId\u0027] \u003d value.getValueAt(row, \"statusId\")\n\t\trow_data[\u0027assignerId\u0027] \u003d value.getValueAt(row, \"assignerId\")\n\t\trow_data[\u0027priority\u0027] \u003d value.getValueAt(row, \"taskPriority\")\n\t\trow_data[\u0027priorityId\u0027] \u003d value.getValueAt(row, \"priorityId\")\n\t\trow_data[\u0027staffName\u0027] \u003d value.getValueAt(row, \"staffName\")\n\t\trow_data[\u0027assignerName\u0027] \u003d value.getValueAt(row, \"assignerName\")\n\t\trow_data[\u0027status\u0027] \u003d value.getValueAt(row, \"Status\")\n\t\tif value.getValueAt(row, \"Completed\") is None:\n\t\t\trow_data[\u0027completed\u0027] \u003d \u0027\u0027\n\t\telse:\n\t\t\trow_data[\u0027completed\u0027] \u003d value.getValueAt(row, \"Completed\")\n\t\t#Append the individual row date to the table data object if loggedInUser \u003d staff/Assigner\n#\t\tsystem.perspective.print(loggedInUser)\n#\t\tsystem.perspective.print(value.getValueAt(row, \"staffId\"))\n#\t\tsystem.perspective.print(value.getValueAt(row, \"assignerId\"))\n\t\t\n\t\t# show all checkbox selected then show all records\n\t\tif showAllTasks \u003d\u003d True:\n\t\t\ttable_data.append(row_data)\n\t\telse:\n\t\t\tif staffOnly \u003d\u003d True:\n\t\t\t\tif loggedInUser \u003d\u003d value.getValueAt(row, \"staffId\"):\n\t\t\t\t\ttable_data.append(row_data)\n\t\t\telse:\n\t\t\t\tif loggedInUser \u003d\u003d value.getValueAt(row, \"staffId\") or loggedInUser \u003d\u003d value.getValueAt(row, \"assignerId\"):\n\t\t\t\t\ttable_data.append(row_data)\n\t\t\n\treturn table_data\n#\treturn value",
                  "type": "script"
                }
              ],
              "type": "expr-struct"
            }
          },
