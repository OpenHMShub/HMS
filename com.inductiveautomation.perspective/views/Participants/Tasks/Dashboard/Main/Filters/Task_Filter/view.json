  "propConfig": {
    "custom.currentSeasonId": {
      "binding": {
        "config": {
          "queryPath": "WinterShelter/getCurrentSeason"
        },
        "transforms": [
          {
            "code": "\treturn value.getValueAt(0,\u0027id\u0027)",
            "type": "script"
          }
        ],
        "type": "query"
      },
      "persistent": true
    },
    "params.activeInActive": {
