    "params.startDateFilter": {
      "binding": {
        "config": {
          "expression": "lookup({/root/year.props.params.options},{/root/year.props.params.selection},None,\u0027id\u0027,\u0027day\u0027)"
        },
        "transforms": [
          {
            "code": "\ttodayDate \u003d system.date.now()\n\tdefaultStartDate \u003d system.date.addYears(todayDate,-1)\n\tif value is not None and value !\u003d \"\":\n\t\tif value \u003d\u003d \"1 Year\":\n\t\t\tpass\n\t\telif value \u003d\u003d \"Yesterday\":\n\t\t\tdefaultStartDate \u003d system.date.addDays(todayDate, -1)\n\t\telif value \u003d\u003d \"7 Days\":\n\t\t\tdefaultStartDate \u003d system.date.addDays(todayDate, -7)\n\t\telif value \u003d\u003d \"14 Days\":\n\t\t\tdefaultStartDate \u003d system.date.addDays(todayDate, -14)\n\t\telif value \u003d\u003d \"Month to Date\":\n\t\t\tdefaultStartDate \u003d system.date.addDays(todayDate, -(system.date.getDayOfMonth(todayDate) -1))\n\t\telif value \u003d\u003d \"30 Days\":\n\t\t\tdefaultStartDate \u003d system.date.addDays(todayDate, -30)\n\t\telif value \u003d\u003d \"60 Days\":\n\t\t\tdefaultStartDate \u003d system.date.addDays(todayDate, -60)\n\t\telif value \u003d\u003d \"90 Days\":\n\t\t\tdefaultStartDate \u003d system.date.addDays(todayDate, -90)\n\t\telif value \u003d\u003d \"120 Days\":\n\t\t\tdefaultStartDate \u003d system.date.addDays(todayDate, -120)\n\t\telif value \u003d\u003d \"Year to Date\":\n\t\t\tdefaultStartDate \u003d system.date.addDays(todayDate, -(system.date.getDayOfYear(todayDate) -1))\n\t\telif value \u003d\u003d \"2 Years\":\n\t\t\tdefaultStartDate \u003d system.date.addYears(todayDate,-2)\n\t\telif value \u003d\u003d \"3 Years\":\n\t\t\tdefaultStartDate \u003d system.date.addYears(todayDate,-3)\n\t\telif value \u003d\u003d \"4 Years\":\n\t\t\tdefaultStartDate \u003d system.date.addYears(todayDate,-4)\n\t\telif value \u003d\u003d \"5 Years\":\n\t\t\tdefaultStartDate \u003d system.date.addYears(todayDate,-5)\n\t\telif value \u003d\u003d \"All Time\":\n\t\t\tdefaultStartDate \u003d system.date.getDate(1970,1,1)\n\n\tdefaultStartDate \u003d system.date.format(defaultStartDate,\u0027yyyy-MM-dd 00:00:00\u0027)\n\treturn defaultStartDate\n\t\t",
            "type": "script"
          }
        ],
        "type": "expr"
      },
      "paramDirection": "inout",
      "persistent": true
    },
    "params.statusId": {
      "binding": {
        "config": {
          "path": "/root/Status.props.params.selection"
        },
        "type": "property"
      },
      "paramDirection": "inout",
      "persistent": true
    }
  },
