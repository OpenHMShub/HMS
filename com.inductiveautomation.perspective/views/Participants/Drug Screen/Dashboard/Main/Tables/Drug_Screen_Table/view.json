        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "../Table.props.data"
              },
              "transforms": [
                {
                  "code": "\tif value \u003d\u003d None:\n\t\t\toutput \u003d \u0027No Rows\u0027\n\telif value.getRowCount() \u003d\u003d 0:\n\t\toutput \u003d \u0027No Rows\u0027\n\telse:\n\t\toutput \u003d str(value.getRowCount()) + \u0027 Rows\u0027\n\treturn output",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "Background/Alternative Text/Error_Message"
          }
        },
        "type": "ia.display.label"
      },
      {
        "events": {
          "component": {
            "onRowClick": {
              "config": {
                "script": "\timport traceback\n\ttry:\n\t\tselectedColumn \u003d self.props.selection.selectedColumn\n\t\tparticipantID \u003d self.props.data.getValueAt(event.row,\u0027participantId\u0027)\n\t\tisActive \u003d self.props.data.getValueAt(event.row,\u0027isActive\u0027)\n\t\tsystem.perspective.print(selectedColumn)\n\t\t#system.perspective.print(participantID)\n\t\tif selectedColumn \u003d\u003d \"participantId\": \n\t\t\t#Display the detail screen\n\t\t\t#system.perspective.print(\u0027Participant Detail\u0027)\n\t\t\ttab_id \u003d 70\n\t\t\tif isActive \u003d\u003d 1:\n\t\t\t\tsystem.perspective.navigate(\"Participants/Detail/\" + str(participantID) + \"/\" + str(tab_id))\n\t\t#elif selectedColumn \u003d\u003d \"Options\":\n\t\telif selectedColumn \u003d\u003d \"id\":\n\t\t\tid \u003d \"PreviewParticipant\" \n\t\t\tparams \u003d {\"participantID\":participantID,\u0027isActive\u0027:isActive}\n\t\t\tsystem.perspective.openDock(id\u003did,params\u003dparams)\n\t\telse:\n\t\t\tpass\n\texcept:\n\t\tlogger \u003d system.util.getLogger(\"Participants DrugScreen Dashboard Table OnRowClick\")\n\t\tmsg \u003d traceback.format_exc()\n\t\tlogger.error(msg)"
              },
              "scope": "G",
              "type": "script"
            }
          }
        },
        "meta": {
          "name": "Table"
        },
        "position": {
          "basis": "100%"
        },
        "propConfig": {
          "custom.sqlData": {
            "access": "PRIVATE",
            "binding": {
              "config": {
                "parameters": {
                  "activity_range": "{view.params.activity_range}",
                  "administeredById": "{session.custom.filters.drugScreenDashboard.administeredById.where}",
                  "drugScreenResonId": "{session.custom.filters.drugScreenDashboard.drugScreenReasonId.where}",
                  "drugScreenTypeId": "{session.custom.filters.drugScreenDashboard.drugScreenTypeId.where}",
                  "firstname": "{session.custom.filters.drugScreenDashboard.FirstName.where}",
                  "lastname": "{session.custom.filters.drugScreenDashboard.LastName.where}",
                  "middlename": "{session.custom.filters.drugScreenDashboard.MiddleName.where}",
                  "nickname": "{session.custom.filters.drugScreenDashboard.NickName.where}",
                  "passed": "{session.custom.filters.drugScreenDashboard.passed.where}",
                  "search": "{session.custom.filters.drugScreenDashboard.Search.where}",
                  "testDate": "{session.custom.filters.drugScreenDashboard.testDate.where}",
                  "veteran": "{session.custom.filters.drugScreenDashboard.Veteran.where}"
                },
                "queryPath": "Participants/DrugScreen/DrugScreenDashboardSelect"
              },
              "type": "query"
            },
            "onChange": {
              "enabled": null,
              "script": "\timport traceback\n\t\n\ttry:\n\t\tdataset \u003d currentValue.value\n\t\t\n\t\tif dataset \u003d\u003d None:\n\t\t\t#system.perspective.sendMessage(\"newParticipantsReferralsMonthly\", payload \u003d {\"newParticipantsReferralsMonthly\": 0}, scope \u003d \"page\")\n\t\t\treturn\n\t\t\n\t\t#Get selected participants\n\t\tidlist \u003d []\n\t\tuniqueCount \u003d 0\n\t\tpassCount \u003d 0\n\t\tnumRows \u003d dataset.getRowCount()\n\t\tfor\ti in range(numRows):\n\t\t\tparticipantID \u003d str(dataset.getValueAt(i, \"participantId\"))\n\t\t\tif participantID not in idlist:\n\t\t\t\tuniqueCount +\u003d 1\n\t\t\tif dataset.getValueAt(i, \"passed\"):\n\t\t\t\tpassCount +\u003d 1\n\t\t\tidlist.append(participantID)\n\t\tif (numRows \u003e 0):\n\t\t\tpctPass \u003d 100 * passCount / numRows\n\t\telse:\n\t\t\tpctPass \u003d 0\n\t\t\t\n\t\tstrIdList \u003d \u0027,\u0027.join(idlist)\n\t\tparams \u003d {\"IdList\": strIdList}\n\n\t\t#KPI3 Interviews Monthly\n\t\tresult3 \u003d system.db.runNamedQuery(\"Participants/DrugScreen/KPI/DrugScreensMonthly\", params)\n\t\tMonthCount \u003d 6\n\t\tdrugScreenMonthly \u003d []\n\t\tnow \u003d system.date.now()\n\t\tfor i in reversed(range(MonthCount)):\n\t\t\timonthago \u003d system.date.addMonths(now, -i)\n\t\t\tcmonthago \u003d system.date.addMonths(imonthago, 1)\n\t\t\tmonth \u003d system.date.getMonth(imonthago) + 1\n\t\t\tdateMonth \u003d system.date.getMonth(imonthago)\n\t\t\tdateYear \u003d system.date.getYear(imonthago)\n\t\t\tdate \u003d system.date.getDate(dateYear, dateMonth, 01)\n\t\t\tdatetime \u003d system.date.setTime(date, 00, 00, 00)\n\t\t\tappend_item \u003d {\"time\": system.date.format(datetime, \"M/d/yyyy\") , \"Participants\": 0}\n\t\t\tresultMonths \u003d 0\n\t\t\tresultPassed \u003d 0\n\t\t\tfor row in range(result3.getRowCount()):\n\t\t\t\tresultMonth \u003d result3.getValueAt(row, \"passed_month\")\n\t\t\t\tif month \u003d\u003d resultMonth:\n\t\t\t\t\tif bool(result3.getValueAt(row, \"passed\")):\n\t\t\t\t\t\tresultPassed +\u003d 1\n\t\t\t\t\tresultMonths +\u003d 1\n\t\t\t\n\t\t\tif resultMonths \u003e 0:\n\t\t\t\tpassedPct \u003d 100 * resultPassed / resultMonths\n\t\t\telse:\n\t\t\t\tpassedPct \u003d 0\n\t\t\tappend_item[\"Participants\"] \u003d passedPct\n\t\t\tdrugScreenMonthly.append(append_item)\n\n\t\t\t#Update Applicants Per Month KPIs\n\t\t\tsystem.perspective.sendMessage(\"drugScreenUpdate\", payload \u003d {\"drugScreenMonthly\": drugScreenMonthly, \"drugScreenCnt\": numRows, \"uniqueCount\": uniqueCount, \"pctPass\": pctPass}, scope \u003d \"page\")\n\t\t\t\t\n\texcept:\n\t\tlogger \u003d system.util.getLogger(\"Participants Referral Dashboard Table custom.SQLData KPI\")\n\t\tmsg \u003d traceback.format_exc()\n\t\tlogger.error(msg)"
            }
          },
          "props.data": {
            "binding": {
              "config": {
                "path": "this.custom.sqlData"
              },
              "type": "property"
            }
          }
