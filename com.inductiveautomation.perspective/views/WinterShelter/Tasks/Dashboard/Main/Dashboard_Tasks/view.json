  "events": {
    "system": {
      "onStartup": {
        "config": {
          "script": "\tloggedInStaff \u003d self.session.custom.loggedInStaffEmployeeID\n\tgetHumanIfFromStaff \u003d system.db.runPrepQuery(\"select * from [staff].[Employee] where id \u003d \"+str(loggedInStaff))\n\tif getHumanIfFromStaff.getRowCount()\u003e0:\n\t\tloggedInStaffHumanId \u003d getHumanIfFromStaff.getValueAt(0,\u0027humanId\u0027)\n\telse:\n\t\tloggedInStaffHumanId \u003d -1\n\tself.getChild(\"root\").getChild(\"Table\").props.params.selectedRowData.assignerId \u003d loggedInStaffHumanId\n\tself.getChild(\"root\").getChild(\"Table\").props.params.selectedRowData.Created \u003d system.date.format(system.date.now(),\u0027MM/dd/YYYY hh:mm a\u0027)"
        },
        "scope": "G",
        "type": "script"
      }
    }
